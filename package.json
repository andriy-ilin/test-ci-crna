{
  "main": "node_modules/expo/AppEntry.js",
  "name": "ukrainer.apl",
  "version": "0.23.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/andriy-ilin/ukrainer.apl"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test:watch": "npx jest-expo --ci --watchAll  --runInBand",
    "test": "npx jest-expo --ci",
    "release": "standard-version -m \"chore(release): publish %s [skip ci]\""
  },
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "**/?(*.)test.js?(x)"
    ],
    "setupTestFrameworkScriptFile": "./setupJest.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "!node_modules/react-runtime"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "expo": "^35.0.0",
    "expo-asset": "~7.0.0",
    "expo-blur": "~7.0.0",
    "expo-font": "~7.0.0",
    "expo-linear-gradient": "~7.0.0",
    "expo-localization": "^7.0.0",
    "expo-permissions": "~7.0.0",
    "firebase": "^7.11.0",
    "i18next": "^13.1.2",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "react": "^16.8.6",
    "react-i18next": "^9.0.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-maps": "~0.25.0",
    "react-native-masked-text": "^1.11.0",
    "react-native-screens": "~1.0.0-alpha.23",
    "react-native-swiper": "^1.5.14",
    "react-navigation": "^3.0.9",
    "transliteration": "1.6.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-preset-expo": "^5.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "expo-cli": "^3.13.8",
    "husky": "^1.3.1",
    "jest-enzyme": "^7.0.1",
    "jest-expo": "^37.0.0",
    "jsdom": "^13.1.0",
    "react-dom": "^16.7.0",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "^16.7.0",
    "schedule": "^0.4.0",
    "standard-version": "^7.1.0"
  }
}
